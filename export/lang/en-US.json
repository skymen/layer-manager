{
  "languageTag": "en-US",
  "fileDescription": "Strings for skymen_layer_manager.",
  "text": {
    "plugins": {
      "skymen_layer_manager": {
        "name": "Layer Manager",
        "description": "Create, Destroy and Reorder Layers at runtime",
        "help-url": "https://www.construct.net",
        "aceCategories": {
          "general": "General"
        },
        "properties": {},
        "actions": {
          "CreateLayer": {
            "list-name": "Create Layer",
            "display-text": "Create layer [i]{0}[/i] ([i]{1}[/i], [i]{2}[/i], [i]{3}[/i], [i]{4}[/i])",
            "description": "Create a layer",
            "params": {
              "name": {
                "name": "Name",
                "desc": "The name of the layer"
              },
              "useRenderCells": {
                "name": "Use Render Cells",
                "desc": "Whether to use render cells"
              },
              "renderAs3D": {
                "name": "Render As 3D",
                "desc": "Whether to render as 3D"
              },
              "useCameraDistanceDrawOrder": {
                "name": "Use Camera Distance Draw Order",
                "desc": "Whether to use camera distance draw order"
              },
              "temporary": {
                "name": "Temporary",
                "desc": "Whether the layer is temporary (will be destroyed when the layout ends)"
              }
            }
          },
          "CreateLayerOnLayout": {
            "list-name": "Create Layer On Layout",
            "display-text": "Create layer [i]{1}[/i] on layout [i]{0}[/i] ([i]{2}[/i], [i]{3}[/i], [i]{4}[/i])",
            "description": "Create a layer on a layout",
            "params": {
              "layout": {
                "name": "Layout",
                "desc": "The layout to create the layer on"
              },
              "name": {
                "name": "Name",
                "desc": "The name of the layer"
              },
              "useRenderCells": {
                "name": "Use Render Cells",
                "desc": "Whether to use render cells"
              },
              "renderAs3D": {
                "name": "Render As 3D",
                "desc": "Whether to render as 3D"
              },
              "useCameraDistanceDrawOrder": {
                "name": "Use Camera Distance Draw Order",
                "desc": "Whether to use camera distance draw order"
              }
            }
          },
          "DestroyLayer": {
            "list-name": "Destroy Layer",
            "display-text": "Destroy layer [i]{0}[/i]",
            "description": "Destroy a layer",
            "params": {
              "name": {
                "name": "Name",
                "desc": "The name of the layer"
              }
            }
          },
          "DestroyLayerOnLayout": {
            "list-name": "Destroy Layer On Layout",
            "display-text": "Destroy layer [i]{1}[/i] on layout [i]{0}[/i]",
            "description": "Destroy a layer on a layout",
            "params": {
              "layout": {
                "name": "Layout",
                "desc": "The layout to destroy the layer on"
              },
              "name": {
                "name": "Name",
                "desc": "The name of the layer"
              }
            }
          },
          "MoveLayerToLayer": {
            "list-name": "Move Layer To Layer",
            "display-text": "Move layer [i]{0}[/i] to layer [i]{1}[/i] (before: [i]{2}[/i])",
            "description": "Move a layer to another layer",
            "params": {
              "name": {
                "name": "Name",
                "desc": "The name of the layer"
              },
              "target": {
                "name": "Target",
                "desc": "The target layer"
              },
              "before": {
                "name": "Before",
                "desc": "Whether to move the layer before the target layer"
              }
            }
          },
          "SetLayerAsChildOfLayer": {
            "list-name": "Set Layer As Child Of Layer",
            "display-text": "Set layer [i]{0}[/i] as child of layer [i]{1}[/i] at index [i]{2}[/i]",
            "description": "Set a layer as a child of another layer at an index",
            "params": {
              "name": {
                "name": "Name",
                "desc": "The name of the layer"
              },
              "target": {
                "name": "Target",
                "desc": "The target layer"
              },
              "index": {
                "name": "Index",
                "desc": "The index to set the layer as a child of the target layer"
              }
            }
          },
          "SetLayerAsRoot": {
            "list-name": "Set Layer As Root",
            "display-text": "Set layer [i]{0}[/i] as root at index [i]{1}[/i]",
            "description": "Set a layer as a root layer at an index",
            "params": {
              "name": {
                "name": "Name",
                "desc": "The name of the layer"
              },
              "index": {
                "name": "Index",
                "desc": "The index to set the layer as a root layer"
              }
            }
          },
          "SetLayerAsRootOnLayout": {
            "list-name": "Set Layer As Root On Layout",
            "display-text": "Set layer [i]{1}[/i] as root on layout [i]{0}[/i] at index [i]{2}[/i]",
            "description": "Set a layer as a root layer on a layout at an index",
            "params": {
              "layout": {
                "name": "Layout",
                "desc": "The layout to set the layer as a root layer on"
              },
              "name": {
                "name": "Name",
                "desc": "The name of the layer"
              },
              "index": {
                "name": "Index",
                "desc": "The index to set the layer as a root layer"
              }
            }
          },
          "SetLayerAsChildOfLayerOnLayout": {
            "list-name": "Set Layer As Child Of Layer On Layout",
            "display-text": "Set layer [i]{1}[/i] as child of layer [i]{2}[/i] on layout [i]{0}[/i] at index [i]{3}[/i]",
            "description": "Set a layer as a child of another layer on a layout at an index",
            "params": {
              "layout": {
                "name": "Layout",
                "desc": "The layout to set the layer as a child of another layer on"
              },
              "name": {
                "name": "Name",
                "desc": "The name of the layer"
              },
              "target": {
                "name": "Target",
                "desc": "The target layer"
              },
              "index": {
                "name": "Index",
                "desc": "The index to set the layer as a child of the target layer"
              }
            }
          }
        },
        "conditions": {
          "IsLayerChildOfLayer": {
            "list-name": "Is Layer Child Of Layer",
            "display-text": "Is layer [i]{0}[/i] child of layer [i]{1}[/i]",
            "description": "Test if a layer is a child of another layer",
            "params": {
              "childLayer": {
                "name": "Child Layer",
                "desc": "The child layer"
              },
              "parentLayer": {
                "name": "Parent Layer",
                "desc": "The parent layer"
              }
            }
          },
          "IsLayerRoot": {
            "list-name": "Is Layer Root",
            "display-text": "Is layer [i]{0}[/i] root",
            "description": "Test if a layer is a root layer",
            "params": {
              "layer": {
                "name": "Layer",
                "desc": "The layer"
              }
            }
          },
          "LayerExists": {
            "list-name": "Layer Exists",
            "display-text": "Layer [i]{0}[/i] exists",
            "description": "Test if a layer exists",
            "params": {
              "layer": {
                "name": "Layer",
                "desc": "The layer"
              }
            }
          },
          "LayerHasChildren": {
            "list-name": "Layer Has Children",
            "display-text": "Layer [i]{0}[/i] has children",
            "description": "Test if a layer has children",
            "params": {
              "layer": {
                "name": "Layer",
                "desc": "The layer"
              }
            }
          },
          "IsLayerChildOfLayerOnLayout": {
            "list-name": "Is Layer Child Of Layer On Layout",
            "display-text": "Is layer [i]{1}[/i] child of layer [i]{2}[/i] on layout [i]{0}[/i]",
            "description": "Test if a layer is a child of another layer on a layout",
            "params": {
              "layout": {
                "name": "Layout",
                "desc": "The layout"
              },
              "childLayer": {
                "name": "Child Layer",
                "desc": "The child layer"
              },
              "parentLayer": {
                "name": "Parent Layer",
                "desc": "The parent layer"
              }
            }
          },
          "IsLayerRootOnLayout": {
            "list-name": "Is Layer Root On Layout",
            "display-text": "Is layer [i]{1}[/i] root on layout [i]{0}[/i]",
            "description": "Test if a layer is a root layer on a layout",
            "params": {
              "layout": {
                "name": "Layout",
                "desc": "The layout"
              },
              "layer": {
                "name": "Layer",
                "desc": "The layer"
              }
            }
          },
          "LayerExistsOnLayout": {
            "list-name": "Layer Exists On Layout",
            "display-text": "Layer [i]{1}[/i] exists on layout [i]{0}[/i]",
            "description": "Test if a layer exists on a layout",
            "params": {
              "layout": {
                "name": "Layout",
                "desc": "The layout"
              },
              "layer": {
                "name": "Layer",
                "desc": "The layer"
              }
            }
          },
          "LayerHasChildrenOnLayout": {
            "list-name": "Layer Has Children On Layout",
            "display-text": "Layer [i]{1}[/i] has children on layout [i]{0}[/i]",
            "description": "Test if a layer has children on a layout",
            "params": {
              "layout": {
                "name": "Layout",
                "desc": "The layout"
              },
              "layer": {
                "name": "Layer",
                "desc": "The layer"
              }
            }
          },
          "IsLayerSublayerOfLayer": {
            "list-name": "Is Layer Sublayer Of Layer",
            "display-text": "Is layer [i]{0}[/i] [i]{2}[/i] sublayer of layer [i]{1}[/i]",
            "description": "Test if a layer is a sublayer of another layer",
            "params": {
              "sub": {
                "name": "Sub",
                "desc": "The sub layer"
              },
              "root": {
                "name": "Root",
                "desc": "The root layer"
              },
              "which": {
                "name": "Which",
                "desc": "Wether to test if the sub layer is any sublayer or a direct sublayer",
                "items": {
                  "any": "Any",
                  "own": "Own"
                }
              }
            }
          },
          "IsLayerSublayerOfLayerOnLayout": {
            "list-name": "Is Layer Sublayer Of Layer",
            "display-text": "Is layer [i]{0}[/i] [i]{2}[/i] sublayer of layer [i]{1}[/i] on layout [i]{3}[/i]",
            "description": "Test if a layer is a sublayer of another layer on a layout",
            "params": {
              "sub": {
                "name": "Sub",
                "desc": "The sub layer"
              },
              "root": {
                "name": "Root",
                "desc": "The root layer"
              },
              "which": {
                "name": "Which",
                "desc": "Wether to test if the sub layer is any sublayer or a direct sublayer",
                "items": {
                  "any": "Any",
                  "own": "Own"
                }
              },
              "layout": {
                "name": "Layout",
                "desc": "The layout"
              }
            }
          },
          "ObjectIsOnLayer_Parent": {
            "list-name": "Object Is On Parent Layer of",
            "display-text": "{0} | Is on {2} parent layer of {1} ({3})",
            "description": "Pick all objects on a given layer.",
            "params": {
              "object": {
                "name": "Object",
                "desc": "The object"
              },
              "layer": {
                "name": "Layer",
                "desc": "The layer"
              },
              "which": {
                "name": "Which",
                "desc": "Wether to test if the sub layer is any sublayer or a direct sublayer",
                "items": {
                  "any": "Any",
                  "own": "Own"
                }
              },
              "self": {
                "name": "Self",
                "desc": "Whether to test if the object is on the layer itself",
                "items": {
                  "yes": "Self included",
                  "no": "Self excluded"
                }
              }
            }
          },
          "ObjectIsOnLayer_Sublayer": {
            "list-name": "Object Is On Sublayer Of",
            "display-text": "{0} | Is on {2} sublayer of {1} ({3})",
            "description": "Pick all objects on a given layer.",
            "params": {
              "object": {
                "name": "Object",
                "desc": "The object"
              },
              "layer": {
                "name": "Layer",
                "desc": "The layer"
              },
              "which": {
                "name": "Which",
                "desc": "Wether to test if the sub layer is any sublayer or a direct sublayer",
                "items": {
                  "any": "Any",
                  "own": "Own"
                }
              },
              "self": {
                "name": "Self",
                "desc": "Whether to test if the object is on the layer itself",
                "items": {
                  "yes": "Self included",
                  "no": "Self excluded"
                }
              }
            }
          }
        },
        "expressions": {
          "RootLayerCount": {
            "translated-name": "RootLayerCount",
            "description": "Get the number of root layers",
            "params": {}
          },
          "SubLayerCount": {
            "translated-name": "SubLayerCount",
            "description": "Get the number of sub layers",
            "params": {
              "root": {
                "name": "Root",
                "desc": "The root layer"
              }
            }
          },
          "SubLayerAt": {
            "translated-name": "SubLayerAt",
            "description": "Get the name of a sub layer",
            "params": {
              "root": {
                "name": "Root",
                "desc": "The root layer"
              },
              "index": {
                "name": "Index",
                "desc": "The index of the sub layer"
              }
            }
          },
          "RootLayerAt": {
            "translated-name": "RootLayerAt",
            "description": "Get the name of a root layer",
            "params": {
              "index": {
                "name": "Index",
                "desc": "The index of the root layer"
              }
            }
          },
          "RootLayerCountOnLayout": {
            "translated-name": "RootLayerCountOnLayout",
            "description": "Get the number of root layers on a layout",
            "params": {
              "layout": {
                "name": "Layout",
                "desc": "The layout"
              }
            }
          },
          "SubLayerCountOnLayout": {
            "translated-name": "SubLayerCountOnLayout",
            "description": "Get the number of sub layers on a layout",
            "params": {
              "layout": {
                "name": "Layout",
                "desc": "The layout"
              },
              "root": {
                "name": "Root",
                "desc": "The root layer"
              }
            }
          },
          "SubLayerAtOnLayout": {
            "translated-name": "SubLayerAtOnLayout",
            "description": "Get the name of a sub layer on a layout",
            "params": {
              "layout": {
                "name": "Layout",
                "desc": "The layout"
              },
              "root": {
                "name": "Root",
                "desc": "The root layer"
              },
              "index": {
                "name": "Index",
                "desc": "The index of the sub layer"
              }
            }
          },
          "RootLayerAtOnLayout": {
            "translated-name": "RootLayerAtOnLayout",
            "description": "Get the name of a root layer on a layout",
            "params": {
              "layout": {
                "name": "Layout",
                "desc": "The layout"
              },
              "index": {
                "name": "Index",
                "desc": "The index of the root layer"
              }
            }
          }
        }
      }
    }
  }
}